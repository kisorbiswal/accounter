<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel">
			<g:Label ui:field="expensesInfo" />
			<g:HTML wordWrap="true" ui:field="recommendedInfo" />
			<g:ScrollPanel  height="300px" styleName="review_expense_account">
				<g:FlexTable ui:field="accountsTable"/>
			</g:ScrollPanel>
			<g:HorizontalPanel>
				<g:Button enabled="true" ui:field="restoreButton" />
				<g:Button ui:field="selectAllButton"/>
			</g:HorizontalPanel>
			<g:HTML wordWrap="true" ui:field="expensesLink" styleName="expense_question_link" />
			<g:HTML wordWrap="true" ui:field="expensesNote" />
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 