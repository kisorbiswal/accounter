<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label styleName="setup_label" ui:field="headerLabel"
			styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel">
			<g:RadioButton name="trackEmployee" ui:field="trackEmployeeYes"
				styleName="employee_yesno" />
			<g:FlowPanel styleName="check_label">
				<g:CheckBox styleName="setup_check_box" ui:field="w2Employees" />
				<g:CheckBox styleName="setup_check_box" ui:field="contractors" />
			</g:FlowPanel>
			<g:RadioButton name="trackEmployee" ui:field="trackEmployeeNo"
				styleName="employee_yesno" />
		</g:FlowPanel>
		<g:FlowPanel ui:field="trackPanel" styleName="track_emp_vpanel">
			<g:Label styleName="setup_label" wordWrap="true" ui:field="trackExpenses"
				styleName="setup_header_label" />
			<g:RadioButton name="trackEmployeeType" ui:field="trackEmployeeExpenseYes"
				styleName="employee_yesno" />
			<g:RadioButton name="trackEmployeeType" ui:field="trackEmployeeExpenseNo"
				styleName="employee_yesno" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 