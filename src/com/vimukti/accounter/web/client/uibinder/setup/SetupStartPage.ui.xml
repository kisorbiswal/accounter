<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel" styleName="setupstartpagepanel">
			<g:Label ui:field="setupInfo" wordWrap="true" styleName="setup_Info"></g:Label>
			<g:FlowPanel>
				<g:FlowPanel styleName="skip_setup">
					<g:FlowPanel>
						<g:Label styleName="setup_label" ui:field="skipSetupInfo"
							wordWrap="true"></g:Label>
					</g:FlowPanel>
					<g:FlowPanel>
						<g:Button ui:field="skipButton"></g:Button>
					</g:FlowPanel>
				</g:FlowPanel>
				<g:FlowPanel styleName="start_setup">
					<g:FlowPanel>
						<g:Label styleName="setup_label" ui:field="startSetupInfo"
							wordWrap="true"></g:Label>
					</g:FlowPanel>
					<g:FlowPanel>
						<g:Button ui:field="startButton"></g:Button>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
			<g:Button ui:field="cancelBtn" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 