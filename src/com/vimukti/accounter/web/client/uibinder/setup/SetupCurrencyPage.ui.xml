<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel styleName="select_grid">
			<g:FlowPanel styleName="primarycurreny-panel">
				<g:Label styleName="setup_label" ui:field="primaryCurrenyLabel" />
				<g:ListBox visibleItemCount="1" ui:field="primaryCurrencyListBox"
					styleName="currency_listbox" />
			</g:FlowPanel>
		</g:FlowPanel>
		<g:CheckBox styleName="setup_check_box" ui:field="isMultiCurrencyAllowed" />
		<!-- <g:VerticalPanel ui:field="currencyListGridPanel"/> -->
	</g:FlowPanel>
</ui:UiBinder> 