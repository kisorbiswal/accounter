<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel" width="100%" height="100%">
			<g:HTML ui:field="customizeAccounter" styleName="customize_Accounter_labels">
			</g:HTML>
			<g:FlowPanel ui:field="industryAccountsPanel" styleName="industry_subindustry_panel">
				<g:FlowPanel styleName="industry_list_table">
					<g:Label ui:field="selectIndustryhead" styleName="select_industry" />
					<g:ListBox ui:field="industryList" visibleItemCount="15"
						styleName="industry_list" />
				</g:FlowPanel>
				<g:FlowPanel styleName="sub_industry_listpanel">
					<g:HTML ui:field="accountsLabel" styleName="accounts_label" />
					<g:ScrollPanel styleName="subindustrylist" height="250px"
						width="250px">
						<g:FlowPanel ui:field="accountsPanel" />
					</g:ScrollPanel>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 